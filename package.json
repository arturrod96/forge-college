    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.3.0",
    "framer-motion": "^12.23.12",
    "input-otp": "^1.2.4",
    "lenis": "^1.3.8",
    "lucide-react": "^0.462.0",
